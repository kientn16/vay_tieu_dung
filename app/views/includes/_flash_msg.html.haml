- if flash[:error]
  .alert.alert-danger
    %strong Error!
    = flash[:error]
- if flash[:warning]
  .alert.alert-warning
    %strong Warning!
    = flash[:warning]
- if flash[:success]
  .alert.alert-success
    %strong Success!
    = flash[:success]
- if notice
  .alert.alert-success
    %strong Success!
    = notice
